{
  "name": "GridPlus",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "clean": "rm -rf node_modules && rm -rf ./ios/build/ModuleCache && rm -rf \"$TMPDIR/react-*\" && watchman watch-del-all && npm cache clean --force",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "ios": "react-native run-ios",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "postinstall": "rn-nodeify --install assert,buffer,crypto,http,https,events,net,path,process,stream,vm --hack",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "start": "react-native start",
    "test:watch": "jest --watch",
    "test": "jest",
    "updateSnapshot": "jest --updateSnapshot",
    "upgrade": "react-native-git-upgrade"
  },
  "dependencies": {
    "apisauce": "^0.14.0",
    "assert": "^1.4.1",
    "bip39": "^2.3.1",
    "bluebird": "^3.5.0",
    "browserify-sign": "^4.0.4",
    "browserify-zlib": "^0.1.4",
    "buffer": "^3.6.0",
    "elliptic": "^6.4.0",
    "ethereumjs-util": "^5.1.2",
    "ethjs-account": "^0.1.1",
    "ethjs-provider-http": "^0.1.6",
    "ethjs-query": "^0.2.6",
    "ethjs-signer": "^0.1.1",
    "events": "^1.1.1",
    "format-json": "^1.0.3",
    "https-browserify": "0.0.1",
    "js-sha3": "^0.6.1",
    "lodash": "^4.17.2",
    "number-to-bn": "^1.7.0",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "prop-types": "^15.5.10",
    "querystringify": "0.0.4",
    "ramda": "^0.24.1",
    "react": "16.0.0-alpha.12",
    "react-native": "0.45.1",
    "react-native-animatable": "^1.2.2",
    "react-native-config": "^0.2.1",
    "react-native-crypto": "^2.0.2",
    "react-native-device-info": "0.10.2",
    "react-native-drawer": "^2.3.0",
    "react-native-elements": "^0.15.0",
    "react-native-fs": "^2.3.3",
    "react-native-http": "github:tradle/react-native-http#834492d",
    "react-native-randombytes": "^2.2.0",
    "react-native-tcp": "^2.0.4",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "^5.0.2",
    "readable-stream": "^1.0.33",
    "redux": "^3.6.0",
    "redux-persist": "^4.1.0",
    "redux-saga": "^0.14.3",
    "reduxsauce": "0.4.1",
    "rlp": "^2.0.0",
    "seamless-immutable": "^7.0.1",
    "steam": "^1.4.0",
    "stream-browserify": "^1.0.0",
    "string_decoder": "^1.0.3",
    "url": "^0.10.3",
    "vm-browserify": "0.0.4",
    "watchman": "^0.1.8"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-jest": "20.0.3",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "2.0.0",
    "enzyme": "^2.6.0",
    "husky": "^0.13.1",
    "ignite-animatable": "^1.0.0",
    "ignite-dev-screens": "^2.0.1",
    "ignite-ir-boilerplate": "^2.0.2",
    "ignite-standard": "^1.0.0",
    "jest": "20.0.4",
    "mockery": "^2.0.0",
    "react-addons-test-utils": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-native-git-upgrade": "^0.2.7",
    "react-test-renderer": "16.0.0-alpha.12",
    "reactotron-react-native": "^1.12.0",
    "reactotron-redux": "^1.11.1",
    "reactotron-redux-saga": "^1.11.1",
    "rn-nodeify": "^7.0.1",
    "snazzy": "^7.0.0",
    "standard": "10.0.2"
  },
  "jest": {
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "vm": "vm-browserify",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "http": "react-native-http",
    "https": "https-browserify",
    "zlib": "browserify-zlib",
    "net": "react-native-tcp",
    "path": "path-browserify"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "vm": "vm-browserify",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "http": "react-native-http",
    "https": "https-browserify",
    "zlib": "browserify-zlib",
    "net": "react-native-tcp",
    "path": "path-browserify"
  }
}
